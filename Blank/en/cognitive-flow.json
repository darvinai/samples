{
  "commands": {
    "NEXT-PAGE": [
      "Next 5"
    ]
  },
  "settings": {
    "invalid-replies": [
      "I am not sure I understood what you said."
    ],
    "general-failure": [
      "We are experiencing technical difficulties at this moment."
    ],
    "silence-timeout": 60
  },
  "conversations": {
    "welcome": {
      "steps": [{
        "type": "message",
        "messages": [
          "This is a welcome conversation for your chatbot."
        ]
      }, {
        "type": "question",
        "entity": "conversation",
        "entity-type": "Conversation",
        "messages": [
          [
            "Here is what I can do for you:"
          ]
        ],
        "display": {
          "type": "quick-reply",
          "data": [
            "Conversation 1",
            "Conversation 2"
          ]
        }
      }]
    },
    "help": {
      "steps": [{
        "type": "message",
        "messages": [
          [
            "If you get stuck, you can always restart our conversation by typing 'restart'",
            "Here is what I can do for you:"
          ]
        ],
        "display": {
          "type": "quick-reply",
          "data": [
            "Conversation 1",
            "Conversation 2"
          ]
        }
      }]
    },
    "restart": {
      "steps": [{
          "type": "message",
          "messages": [
            "Your conversation is restarted."
          ]
        },
        {
          "type": "conversation",
          "conversation": "welcome"
        }
      ]
    },
    "conversationOne": {
      "clear-context": true,
      "steps": [{
        "type": "message",
        "messages": [
          [
            "This is conversation 1"
          ]
        ]
      }]
    },
    "conversationTwo": {
      "clear-context": true,
      "steps": [{
        "type": "message",
        "messages": [
          [
            "This is conversation 2"
          ]
        ]
      }]
    }
  }
}
